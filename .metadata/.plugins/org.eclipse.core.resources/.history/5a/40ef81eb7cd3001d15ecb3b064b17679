package ac;

import java.util.*;

public class Suqen {
	public static boolean isPrime(int x) {
		for(int i=2;i*i<=x;i++) {
			if(x%i==0) {
				return false;
			}
		}
		return true;
	}
	public static void main(String[] args) {
		List<Integer> list=new ArrayList<Integer>();
		System.out.println(isPrime(7));
	}

}
